---
import titleHenry from "../../assets/images/titleHenry.png";
import titleHPucara from "../../assets/images/tituloPucara.jpg";
---

<div class="mt-10" id="education">
  <h3 class="text-2xl font-bold">Educación</h3>
  <div class="flex gap-3 flex-wrap mt-8">
      <div class="flex-1 flex flex-col items-center text-xl font-bold gap-2">
          <h4>Henry</h4>
          <img class="bg-gray-200 rounded-md object-cover h-[250px] min-w-[300px] max-w-[350px] w-[98%] mx-auto" src={titleHenry.src} alt="titulo de henry" />
      </div>
      <div class="flex-1 flex flex-col items-center text-xl font-bold gap-2">
          <h4>Apdes Pucará</h4>
          <img class="bg-gray-200 rounded-md object-cover h-[250px] min-w-[300px] max-w-[350px] w-[98%] mx-auto" src={titleHPucara.src} alt="titulo pucara" />
      </div>
  </div>
</div>